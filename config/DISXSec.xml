<?xml version="1.0" encoding="ISO-8859-1"?>

<alg_conf>

<!--
  Configuration sets for the DISXSec algorithm

  parameter                     comment
  **********************************************************************************
  partial-xsec-alg-name">       d2xsec/dxdy algorithm
  partial-xsec-param-set">      ... and its config set
  integrator-alg-name">         algorithm used for 2-D numerical integration
  integrator-param-set">        ... and its config set
  x-min                         minimum x (default: epsilon)
  x-max                         maximum x (default: 1-epsilon)
  y-min                         minimum y (default: epsilon)
  y-max                         maximum y (default: 1-epsilon)
  Wmin                          kinematical cut in W
  Q2min                         kinematical cut in Q^2
-->

  <param_set name="CC-Charm-Default"> 
     <param type="string" name="partial-xsec-alg-name">  genie::AivazisCharmPXSecLO          </param>
     <param type="string" name="partial-xsec-param-set"> CC-Default                          </param>
     <param type="string" name="integrator-alg-name">    genie::Simpson2D                    </param>
     <param type="string" name="integrator-param-set">   Default-Logarithmic-FixedNBins-101  </param>
     <param type="double" name="Wmin">                   1.600                               </param>
     <param type="double" name="Q2min">                  1.000                               </param>
  </param_set>

  <param_set name="CC-Charm-Slow-Rescaling"> 
     <param type="string" name="partial-xsec-alg-name">  genie::SlowRsclCharmDISPXSecLO      </param>
     <param type="string" name="partial-xsec-param-set"> CC-Default                          </param>
     <param type="string" name="integrator-alg-name">    genie::Simpson2D                    </param>
     <param type="string" name="integrator-param-set">   Default-Logarithmic-FixedNBins-101  </param>
     <param type="double" name="Wmin">                   1.600                               </param>
     <param type="double" name="Q2min">                  1.000                               </param>
  </param_set>

  <param_set name="CC-Bardin-Rad-Corr"> 
     <param type="string" name="partial-xsec-alg-name">  genie::BardinDISPXSec               </param>
     <param type="string" name="partial-xsec-param-set"> CC                                  </param>
     <param type="string" name="integrator-alg-name">    genie::Simpson2D                    </param>
     <param type="string" name="integrator-param-set">   Default-Logarithmic-FixedNBins-101  </param>
     <param type="double" name="Wmin">                   1.600                               </param>
     <param type="double" name="Q2min">                  1.000                               </param>
  </param_set>

  <param_set name="CC-Parton-Model-Default"> 
     <param type="string" name="partial-xsec-alg-name">  genie::DISPartonModelPXSec          </param>
     <param type="string" name="partial-xsec-param-set"> CC-Default                          </param>
     <param type="string" name="integrator-alg-name">    genie::Simpson2D                    </param>
     <param type="string" name="integrator-param-set">   Default-Logarithmic-FixedNBins-101  </param>
     <param type="double" name="Wmin">                   1.600                               </param>
     <param type="double" name="Q2min">                  1.000                               </param>
  </param_set>

  <param_set name="NC-Parton-Model-Default"> 
     <param type="string" name="partial-xsec-alg-name">  genie::DISPartonModelPXSec          </param>
     <param type="string" name="partial-xsec-param-set"> NC-Default                          </param>
     <param type="string" name="integrator-alg-name">    genie::Simpson2D                    </param>
     <param type="string" name="integrator-param-set">   Default-Logarithmic-FixedNBins-101  </param>
     <param type="double" name="Wmin">                   1.600                               </param>
     <param type="double" name="Q2min">                  1.000                               </param>
  </param_set>

</alg_conf>

