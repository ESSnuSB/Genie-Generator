<?xml version="1.0" encoding="ISO-8859-1"?>

<alg_conf>

<!--
  Configuration sets for the DISXSec algorithm

  parameter                     comment
  **********************************************************************************
  partial-xsec-alg-name">       d2xsec/dxdy algorithm
  partial-xsec-param-set">      ... and its config set
  integrator-alg-name">         algorithm used for 2-D numerical integration
  integrator-param-set">        ... and its config set
  x-min                         minimum x (default: epsilon)
  x-max                         maximum x (default: 1-epsilon)
  y-min                         minimum y (default: epsilon)
  y-max                         maximum y (default: 1-epsilon)
  Wmin                          kinematical cut in W
  Q2min                         kinematical cut in Q^2
-->

  <param_set name="CC-Parton-Model-Default"> 
     <param type="alg" name="DiffXSecAlg">  genie::DISPartonModelPXSec/CC-Default               </param>
     <param type="alg" name="Integrator">   genie::Simpson2D/Default-Logarithmic-FixedNBins-101 </param>
  </param_set>

  <param_set name="NC-Parton-Model-Default"> 
     <param type="alg" name="DiffXSecAlg">   genie::DISPartonModelPXSec/NC-Default                </param>
     <param type="alg" name="Integrator">    genie::Simpson2D/Default-Logarithmic-FixedNBins-101  </param>
  </param_set>

  <param_set name="CC-Charm-Default"> 
     <param type="alg"    name="DiffXSecAlg">  genie::AivazisCharmPXSecLO/CC-Default               </param>
     <param type="alg"    name="Integrator">   genie::Simpson2D/Default-Logarithmic-FixedNBins-101 </param>
     <param type="double" name="Kine-Wmin">    1.600                                               </param>
     <param type="double" name="Kine-Q2min">   1.000                                               </param>
  </param_set>

  <param_set name="CC-Charm-Slow-Rescaling"> 
     <param type="alg"    name="DiffXSecAlg">  genie::SlowRsclCharmDISPXSecLO/CC-Default           </param>
     <param type="alg"    name="Integrator">   genie::Simpson2D/Default-Logarithmic-FixedNBins-101 </param>
     <param type="double" name="Kine-Wmin">    1.600                                               </param>
     <param type="double" name="Kine-Q2min">   1.000                                               </param>
  </param_set>

  <param_set name="CC-Bardin-Rad-Corr"> 
     <param type="alg"    name="DiffXSecAlg">  genie::BardinDISPXSec/CC                             </param>
     <param type="alg"    name="Integrator">   genie::Simpson2D/Default-Logarithmic-FixedNBins-101  </param>
     <param type="double" name="Kine-Wmin">    1.600                                                </param>
     <param type="double" name="Kine-Q2min">   1.000                                                </param>
  </param_set>

</alg_conf>

