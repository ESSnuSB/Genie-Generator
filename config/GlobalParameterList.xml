<?xml version="1.0" encoding="ISO-8859-1"?>

<global_param_list>

  <param_set name="Default"> 

     <!-- 
      *********************************************************************       
       CKM quark mixing parameters 
      ********************************************************************* 
      -->
     <param type="double" name="CKM-Vud">  0.9738  </param>
     <param type="double" name="CKM-Vus">  0.2200  </param>
     <param type="double" name="CKM-Vcd">  0.2240  </param>
     <param type="double" name="CKM-Vcs">  0.9960  </param>

     <!-- 
       *********************************************************************       
       Constants in Feynman-Kislinger-Ravndal model for baryon excitation 
       used in the Rein-Seghal resonance model
       *********************************************************************       
     -->
     <param type="double" name="RS-Omega"> 1.05 </param>
     <param type="double" name="RS-Zeta">  0.75 </param>

     <!-- 
       *********************************************************************       
       Constants in the Bodek-Yang DIS structure function model
       and in the Bodek-Yang GRV-LO-9* PDF corrections
       *********************************************************************       
     -->
     <param type="double" name="BY-A">     0.538   </param>
     <param type="double" name="BY-B">     0.305   </param>
     <param type="double" name="BY-CsU">   0.363   </param>
     <param type="double" name="BY-CsD">   0.255   </param>
     <param type="double" name="BY-Cv1U">  0.291   </param>
     <param type="double" name="BY-Cv2U">  0.189   </param>
     <param type="double" name="BY-Cv1D">  0.202   </param>
     <param type="double" name="BY-Cv2D">  0.255   </param>
     <param type="double" name="BY-X0">   -0.00817 </param>
     <param type="double" name="BY-X1">    0.0506  </param>
     <param type="double" name="BY-X2">    0.0798  </param>

     <!-- 
       *********************************************************************       
       Minimum Q2 for PDF evaluation - PDF evolution frozen for Q2<Q2min
       *********************************************************************       
     -->
     <param type="double" name="PDF-Q2min"> 0.800 </param>

     <!-- 
       *********************************************************************       
       Switches controlling the DIS structure function calculation.
       Those switches specify whether to:
       - include an R (~FL) factor into the calculation
       - include a nuclear factor (accounting for shadowing/anti-shadowing/.)
       - correct the computed F3 using the estimated R factor
       *********************************************************************       
     -->
     <param type="bool" name="DISSF-IncludeR">       true </param>
     <param type="bool" name="DISSF-IncludeNuclMod"> true </param>
     <param type="bool" name="DISSF-CorrectF3WithR"> true </param>

     <!-- 
       *********************************************************************       
       Proton and neutron anomalous magnetic moments 
       *********************************************************************       
     -->
     <param type="double" name="AnomMagnMoment-P">   2.7928473  </param>
     <param type="double" name="AnomMagnMoment-N">  -1.913042   </param>

     <!-- 
       *********************************************************************       
       Axial and vector masses for elastic scattering  
       *********************************************************************       
     -->
     <param type="double" name="EL-Ma">  1.032  </param>
     <param type="double" name="EL-Mv">  0.840  </param> 

     <!-- 
       *********************************************************************       
       Axial and vector masses for quasi-elastic scattering  
       *********************************************************************       
     -->
     <param type="double" name="QEL-Ma"> 1.032 </param>
     <param type="double" name="QEL-Mv"> 0.840 </param>

     <!-- 
       *********************************************************************       
       Axial and vector masses for resonance production  
       *********************************************************************       
     -->
     <param type="double" name="RES-Ma"> 1.032 </param>
     <param type="double" name="RES-Mv"> 0.840 </param>

     <!-- 
       *********************************************************************       
       Axial mass for coherent scattering 
       *********************************************************************       
      -->
     <param type="double" name="COH-Ma"> 1.000 </param>

     <!-- 
       *********************************************************************       
       Value of axial form factor at Q2=0  
       *********************************************************************       
     -->
     <param type="double" name="QEL-FA0"> -1.260 </param>

     <!-- 
       *********************************************************************       
       Parameter eta controlling the strange axial form factor 
       *********************************************************************       
     -->
     <param type="double" name="EL-Axial-Eta"> 0.12 </param>

     <!-- 
       *********************************************************************       
       Nuclear scale parameter in coherent scattering (in fm)
       *********************************************************************       
     -->
     <param type="double" name="COH-Ro"> 1.000 </param>

     <!-- 
       *********************************************************************       
       Real over imaginary fwd scattering amplitude in coherent interactions 
       *********************************************************************       
     -->
     <param type="double" name="COH-ReImAmpl"> 0.300 </param>

     <!-- 
       *********************************************************************       
       Cv and Ca parameters for neutrino-electron elastic scattering 
       *********************************************************************       
     -->
     <param type="double" name="NuElecEL-CV"> -0.52 </param>
     <param type="double" name="NuElecEL-CA">  0.06 </param>

     <!-- 
       *********************************************************************       
       Cabbibo and Weinberg angles 
       *********************************************************************       
     -->
     <param type="double" name="CabbiboAngle">  0.226893 </param> 
     <param type="double" name="WeinbergAngle"> 0.501568 </param> 

     <!-- 
       *********************************************************************       
       Charm mass 
       *********************************************************************       
     -->
     <param type="double" name="Charm-Mass"> 1.300 </param>

     <!-- 
       *********************************************************************       
       Default list of baryon resonances included in cross section models 
       and generation threads 
       *********************************************************************       
     -->
     <param type="string" name="ResonanceNameList">
         P33(1232),S11(1535),D13(1520),S11(1650),D13(1700),D15(1675),
         S31(1620),D33(1700),P11(1440),P13(1720),F15(1680),P31(1910),
         P33(1920),F35(1905),F37(1950),P11(1710)
     </param>

     <!-- 
       *********************************************************************       
       Data for building the KNO spline 
       *********************************************************************       
     -->
     <param type="string" name="KNO-Distribution"> data/kno/KNO.dat </param>

     <!-- 
       *********************************************************************       
       Parameters controlling the hadronic multiplicities in the KNO model
       *********************************************************************       
     -->
     <param type="double" name="KNO-Beta">      1.30 </param> 
     <param type="double" name="KNO-Alpha-vp">  0.50 </param> 
     <param type="double" name="KNO-Alpha-vn">  0.00 </param> 
     <param type="double" name="KNO-Alpha-vbp"> 0.20 </param> 
     <param type="double" name="KNO-Alpha-vbn"> 0.20 </param> 
     
     <!-- 
       *********************************************************************       
       KNO hadronization model probabilities for producing hadron pairs
     -->
     <param type="double" name="KNO-ProbPi0Pi0">        0.30 </param>
     <param type="double" name="KNO-ProbPiplusPiminus"> 0.60 </param>
     <param type="double" name="KNO-ProbKplusKminus">   0.05 </param>
     <param type="double" name="KNO-ProbK0K0bar">       0.05 </param>

     <!-- 
       *********************************************************************       
       Parameters controlling the PYTHIA hadronization model
      
       ssbar:   [PYTHIA def:0.30    ] [NUX tune:0.21    ]
       pt2:     [PYTHIA def:0.36 GeV] [NUX tune:0.44 GeV]
       pt2tail: [PYTHIA def:0.01    ] [NUX tune:0.01    ]
       cutoff:  [PYTHIA def:0.80 GeV] [NUX tune:0.20 GeV]
       *********************************************************************       
     -->
     <param type="double" name="PYTHIA-SSBarSuppression">      0.30 </param>
     <param type="double" name="PYTHIA-GaussianPt2">           0.36 </param>
     <param type="double" name="PYTHIA-NonGaussianPt2Tail">    0.01 </param>
     <param type="double" name="PYTHIA-RemainingEnergyCutoff"> 0.80 </param>

     <!-- 
       *********************************************************************       
       NEUGEN parameters applied to DIS hadronic multiplicity distributions 
       to avoid 2-ble counting with RES
       *********************************************************************       
     -->
     <param type="double" name="DIS-HMultWgt-vp-CC-m2">  0.000  </param>
     <param type="double" name="DIS-HMultWgt-vp-CC-m3">  1.000  </param>
     <param type="double" name="DIS-HMultWgt-vp-NC-m2">  0.000  </param>
     <param type="double" name="DIS-HMultWgt-vp-NC-m3">  0.387  </param>
     <param type="double" name="DIS-HMultWgt-vn-CC-m2">  0.080  </param>
     <param type="double" name="DIS-HMultWgt-vn-CC-m3">  1.000  </param>
     <param type="double" name="DIS-HMultWgt-vn-NC-m2">  0.000  </param>
     <param type="double" name="DIS-HMultWgt-vn-NC-m3">  0.387  </param>
     <param type="double" name="DIS-HMultWgt-vbp-CC-m2"> 0.240  </param>
     <param type="double" name="DIS-HMultWgt-vbp-CC-m3"> 0.874  </param>
     <param type="double" name="DIS-HMultWgt-vbp-NC-m2"> 0.000  </param>
     <param type="double" name="DIS-HMultWgt-vbp-NC-m3"> 0.133  </param>
     <param type="double" name="DIS-HMultWgt-vbn-CC-m2"> 0.000  </param>
     <param type="double" name="DIS-HMultWgt-vbn-CC-m3"> 0.874  </param>
     <param type="double" name="DIS-HMultWgt-vbn-NC-m2"> 0.000  </param>
     <param type="double" name="DIS-HMultWgt-vbn-NC-m3"> 0.133  </param>

     <!-- 
       *********************************************************************       
       RES/DIS joining parameter Wcut 
       *********************************************************************       
     -->
     <param type="bool"   name="UseDRJoinScheme"> true </param>
     <param type="double" name="Wcut">            1.7  </param>

     <!-- 
       *********************************************************************       
       INTRANUKE parameters 
       Ro (in fm)
       *********************************************************************       
     -->
     <param type="double" name="INUKE-FormationZone"> 2.0  </param>
     <param type="double" name="INUKE-KPt2">          1.0  </param>
     <param type="double" name="INUKE-Ro">            1.2  </param>

  </param_set>

</global_param_list>

