<?xml version="1.0" encoding="ISO-8859-1"?>

<alg_conf>

<!--
Configuration for the QELXSec xsec algorithm.

Configurable Parameters:
.....................................................................................................
Name               Type     Optional   Comment                          Default
.....................................................................................................
DiffXSecAlg        alg      No
Integrator         alg      No
-->

  <param_set name="CC-Default"> 
     <param type="alg" name="DiffXSecAlg">  genie::QELPXSec/CC-Default            </param>
     <param type="alg" name="Integrator">   genie::Simpson1D/Default-Logarithmic  </param>
  </param_set>

  <param_set name="NC-Default"> 
     <param type="alg" name="DiffXSecAlg">  genie::NuNucElasticPXSec/Default      </param>
     <param type="alg" name="Integrator">   genie::Simpson1D/Default-Logarithmic  </param>
  </param_set>

  <param_set name="Charm-Kovalenko"> 
     <param type="alg" name="DiffXSecAlg">  genie::KovalenkoQELCharmPXSec/Default </param>
     <param type="alg" name="Integrator">   genie::Simpson1D/Default-Logarithmic  </param>
  </param_set>

</alg_conf>

