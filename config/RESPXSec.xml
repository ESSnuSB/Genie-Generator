<?xml version="1.0" encoding="ISO-8859-1"?>

<alg_conf>

<!--
Configuration for the RESPXSec RES dxsec/dt (where t=Q2,W) cross section algorithm

Configurable Parameters:
..........................................................................................................................
Name              Type     Opt   Comment                                      Default
..........................................................................................................................
DiffXSecAlg       alg      No    Differential xsec algorithm name
Integrator        alg      No    Integration algorithm
-->

  <param_set name="PaschosLalakulich-P33"> 
     <param type="alg" name="DiffXSecAlg"> genie::P33PaschosLalakulichPXSec/Default </param>
     <param type="alg" name="Integrator">  genie::Simpson1D/Default-Logarithmic     </param>
  </param_set>

  <param_set name="ReinSeghal-RSPP"> 
     <param type="alg" name="DiffXSecAlg"> genie::ReinSeghalSPPPXSec/Default        </param>
     <param type="alg" name="Integrator">  genie::Simpson1D/Default-Logarithmic     </param>
  </param_set>

</alg_conf>

