<?xml version="1.0" encoding="ISO-8859-1"?>

<!--

Configuration for the RESXSec cross section algorithm

Configurable Parameters:
..........................................................................................................................
Name              Type     Opt   Comment                                      Default
..........................................................................................................................
DiffXSecAlg       alg      No    Differential xsec algorithm name
Integrator        alg      No    Integration algorithm
Kine-Wmin         double   Yes   Kinematical cut                              Use physical range
Kine-Wmax         double   Yes   Kinematical cut                              Use physical range     
Kine-Q2min        double   Yes   Kinematical cut                              Use physical range   
Kine-Q2max        double   Yes   Kinematical cut                              Use physical range    
-->

<alg_conf>

  <param_set name="PaschosLalakulich-P33"> 
     <param type="alg"    name="DiffXSecAlg">  genie::P33PaschosLalakulichPXSec/Default             </param>
     <param type="alg"    name="Integrator">   genie::Simpson2D/Default-Logarithmic-FixedNBins-Fast </param>
     <param type="double" name="Kine-Wmax">    1.4   </param>
  </param_set>

  <param_set name="ReinSeghal-RSPP"> 
     <param type="alg"    name="DiffXSecAlg">  genie::ReinSeghalSPPPXSec/Default                    </param>
     <param type="alg"    name="Integrator">   genie::Simpson2D/Default-Logarithmic-FixedNBins-Fast </param>
     <param type="double" name="Wmax">         1.4 </param>
  </param_set>

  <param_set name="ReinSeghal-RES"> 
     <param type="alg"    name="DiffXSecAlg">  genie::ReinSeghalRESPXSec/Default                    </param>
     <param type="alg"    name="Integrator">   genie::Simpson2D/Default-Logarithmic-FixedNBins-Fast </param>
     <param type="double" name="Wmax">         1.4 </param>
  </param_set>

</alg_conf>

